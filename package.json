{
  "name": "sentry-example",
  "version": "1.0.0",
  "description": "Sentry memory leak",
  "license": "UNLICENSED",
  "scripts": {
    "build:build-ts": "tsc --sourceMap false",
    "build:clean": "rm -rf ./dist",
    "build": "npm run build:clean && npm run lint && npm run build:build-ts",
    "dev": "node --trace-warnings --inspect -r dotenv/config dist/src/server.js dotenv_config_path=.env",
    "lint": "tsc --noEmit && eslint src"
  },
  "dependencies": {
    "@sentry/node": "^7.102.1",
    "express": "4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.20",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "dotenv": "^16.4.5",
    "eslint": "^8.56.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-plugin-import": "^2.29.1",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 100,
    "trailingComma": "all"
  }
}
